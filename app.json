{
  "expo": {
    "name": "Wealth Manager",
    "slug": "WealthAndAssetManagerMobile",
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "scheme": "wealthmanager",
    "userInterfaceStyle": "automatic",
    "description": "Your personal wealth and asset management companion",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.izien.WealthManager"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.jpg",
        "backgroundColor": "#2E8B57"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.izien.wealthmanager"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2E8B57"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "41f2ee23-268f-4126-aa5b-82dccb838cfc"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/41f2ee23-268f-4126-aa5b-82dccb838cfc"
    }
  }
}
